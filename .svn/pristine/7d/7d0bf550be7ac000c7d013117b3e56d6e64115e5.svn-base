/* Header */
.large-header {
	position: relative;
	width: 100%;
	background: #333;
	overflow: hidden;
	background-size: cover;
	background-position: center center;
	z-index: 1;
}

.demo-1 .large-header {
	background-image: url('/static/Login/img/demo-1-bg.jpg');
}
.logo_box{
	width: 400px;
	height: 500px;
	padding: 35px;
	color: #EEE;
	position: absolute;
	left: 50%;
	top:50%;
	margin-left: -200px;
	margin-top: -250px;
  //background: rgba(22, 88, 144, 0.42);
}
.logo_box h3{
	text-align: center;
	height: 20px;
	font: 20px "microsoft yahei",Helvetica,Tahoma,Arial,"Microsoft jhengHei",sans-serif;
	color: #FFFFFF;
	height: 20px;
	line-height: 20px;
	padding:0 0 35px 0;
}
.forms{
	width: 280px;
	height: 485px;
}
.logon_inof{
	width: 100%;
	min-height: 450px;
	padding-top: 35px;
	position: relative;
}
.input_outer{
	height: 46px;
	padding: 0 5px;
	margin-bottom: 8px;
	border-radius: 50px;
	position: relative;
	border: rgba(255,255,255,0.2) 2px solid !important;
}
.u_user{
	width: 25px;
	height: 25px;
	background: url(/static/Login/img/login_ico.png);
	background-position:  -140px -13px;
	position: absolute;
	margin: 10px 13px;
}
.us_uer{
	width: 25px;
	height: 25px;
	background-image: url(/static/Login/img/login_ico.png);
	background-position: -140px -47px;
	position: absolute;
	margin: 10px 13px;
}
.us_phone{
	width: 30px;
	height: 30px;
	background-image: url(/static/Login/img/login_ico.png);
	background-position: -100px -122px;
	position: absolute; 
	margin: 7px 12px;
}
.us_message{
	width: 30px;
	height: 30px;
	background-image: url(/static/Login/img/login_ico.png);
	background-position: -100px 10px;
	position: absolute; 
	margin: 7px 12px;
}
.l-login{
	position: absolute;
	z-index: 1;
	left: 50px;
	top: 0;
	height: 46px;
	font: 14px "microsoft yahei",Helvetica,Tahoma,Arial,"Microsoft jhengHei";
	line-height: 46px;
}
.text{
	width: 220px;
	height: 46px;
	outline: none;
	display: inline-block;
	font: 14px "microsoft yahei",Helvetica,Tahoma,Arial,"Microsoft jhengHei";
	margin-left: 50px;
	border: none;
	background: none;
	line-height: 46px;
}
/*///*/
.mb2{
	margin-bottom: 20px
}
.mb2 a{
	text-decoration: none;
	outline: none;
}
.mb3{
  margin: 10px 13px;
  height: 25px;
  float: right;
}
.submit {
	padding: 13px;
	margin-top: 20px;
	display: block;
}
.act-but{
	line-height: 20px;
	text-align: center;
	font-size: 20px;
	border-radius: 50px;
	background: #0096e6;
}
.fg{
  padding: 0 50px;
  border-radius: 50px;
  position: relative;
  font-size: 12px;
  height: 20px;
}
.forget:hover{
  color: rgb(60, 201, 245);
  cursor: pointer;
}
.register:hover{
  color: rgb(60, 201, 245);
  cursor: pointer;
}
.forget{
  color: #FFFFFF;
  float:left;
}
.register{
  color: #FFFFFF;
  float:right;
}
.lbox{
  display:none;
}
.active{
  display: block;
}
.backbtn{
  float: left;
  width: 35px;
  height: 35px;
  background: url(/static/Login/img/login_ico.png);
  background-position: -99px -205px;
  position: absolute;
  margin: 12px 13px;
}
.backbtn:hover{
  background-position: -55px 122px;
}
.registerbtn{
  height: 45px;
  position: relative;
  margin-top: -20px;
  padding: 0px 10px;
}
.nextbtn{
  float: right;
  width: 35px;
  height: 35px;
  background: url(/static/Login/img/login_ico.png);
  background-position: -99px -205px;
  margin: 10px 13px;
  transform: rotate(180deg);
}
.nextbtn:hover{
  background-position: -55px 122px;
}
.fg1{
  padding: 0 50px;
  border-radius: 50px;
  position: relative;
  font-size: 10px;
  height: 20px;
}
